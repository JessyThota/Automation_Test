import json

data = {
    a: 0,
    b: 9.6,
    c: "Hello World",
    d: {
        a: 4
    }
}
# The keys are not sorted by default, you would have to add the sort_keys=True argument to json.dumps()
json_data = json.dumps(data, sort_keys=True)
# The reverse can be obtained by using json.loads() which returns a dictionary representation of json.
print(json_data)
